{
    "name": "jmichaelward/hello-gutenberg",
    "description": "Implementation of the Hello World Gutenberg block in a Composer-installable package.",
    "type": "wordpress-block",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.jmichaelward.com"
        }
    ],
    "require": {
        "composer/installers": "*",
        "webdevstudios/oops-wp": "^0.2",
        "jmichaelward/wp-editor-block-installer": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "JMichaelWard\\WPBlock\\": "src/"
        }
    },
    "extra": {
        "installer-name": "hello-gutenberg"
    }
}
